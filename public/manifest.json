{
    "manifest_version": 2,
    "name": "simp.io",
    "version": "0.2.1",
    "description": "Summarizes payments on the YouTube payment page.",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["build/bundle.js"],
            "css": ["lib/css/bare.css"]
        }
    ]
}